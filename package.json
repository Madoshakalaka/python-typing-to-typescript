{
  "name": "python-typing-to-typescript",
  "version": "1.0.5",
  "description": "Convert python TypedDict's and type hints to typescript interface",
  "scripts": {
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "build": "tsc py_ast_json_to_ts.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
  },
  "bin": {
    "pttts": "py_ast_json_to_ts.js",
    "pytticus": "py_ast_json_to_ts.js"
  },
  "keywords": ["transpiler", "python transpiler", "typing", "type", "API", "schema", "typescript", "json schema"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argparse": "^2.0.1",
    "temp": "^0.9.4",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/argparse": "^2.0.4",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.12",
    "@types/temp": "^0.8.34",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1"
  }
}
